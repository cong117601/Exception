package com.yunhe.day02;

public class Test {

	public static void eat() {
		System.out.println("nichi si ");
	}

	public static void main(String[] args) {
		int[] a = { 3, 2, 1, 8 };
		for (int i = 0; i < a.length - 1; i++) {
			for (int j = 0; j < a.length - 1 - i; j++) {
				if (a[i] < a[i + 1]) {
					int b = a[i + 1];
					a[i + 1] = a[i];
					a[i] = b;
				}
			}
for (int c : a) {
	System.out.print(a);
}
		}

	}
}
