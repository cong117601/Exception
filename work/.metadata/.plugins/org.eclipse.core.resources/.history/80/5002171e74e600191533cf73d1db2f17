package com.yunhe.day02;

public class Test {

	public static void eat() {
		System.out.println("nichi si ");
	}

	public static void main(String[] args) {
		int[] a = { 1, 2, 5, 3, 4, 6, 8 };
		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a.length - 1 - i; j++) {
				if (a[j] < a[j + 1]) {
					int b = a[j + 1];
					a[b + 1] = a[j];
					a[j] = b;
				}

			}
			for (int c : a) {
				System.out.println(c);
			}
			
		}
	}
}
